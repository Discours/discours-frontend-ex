{
  "name": "discours-frontend-main",
  "version": "0.0.1",
  "description": "Фронтэнд проекта Discours.io",
  "engines": {
    "node": ">=10.0.0",
    "npm": ">=6.9.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Discours/discours-frontend-main"
  },
  "keywords": [
    "discours.io",
    "frontend",
    "contribute-me",
    "russian",
    "website"
  ],
  "bugs": {
    "url": "https://github.com/Discours/discours-frontend-main/issues"
  },
  "homepage": "https://discours.io",
  "author": "goooseman",
  "license": "GNU GPLv3",
  "private": true,
  "scripts": {
    "pretest": "gettext-utils import-strings",
    "prestart": "gettext-utils import-strings",
    "prebuild": "gettext-utils import-strings",
    "prestorybook": "gettext-utils import-strings",
    "prestorybook-build": "gettext-utils import-strings",
    "prelint": "gettext-utils import-strings",
    "precommit": "gettext-utils export-strings --default-locale=ru && git add src/i18n/*.po* && lint-staged",
    "commit": "commit-prompt --no-verify",
    "lint": "npm run lint:tsc && npm run lint:tslint && npm run lint:stylelint",
    "lint:tsc": "tsc -p tsconfig.json --noEmit",
    "lint:prettier": "prettier --write \"{src,test}/**/*.ts*\"",
    "lint:tslint": "tslint -c tslint.json -p tsconfig.json",
    "lint:stylelint": "stylelint 'src/**/*.css'",
    "start": "env-cmd -e development webpack-dev-server --config webpack.config.js --hot",
    "build": "env-cmd -e production webpack --config webpack.config.js",
    "postbuild": "es-check es5 './www/**/*.js'",
    "serve": "ws --directory www --port 3000 --spa index.html",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "storybook-build": "build-storybook -c .storybook",
    "clean": "rimraf 'node_modules/.cache' '.awcache'",
    "prerelease": "gettext-utils validate-strings",
    "release": "./config/deployment/release.sh",
    "cy:e2e": "cypress run --spec 'cypress/integration/e2e/tests/*'",
    "cy:open": "env-cmd -e development npx cypress open",
    "test": "jest --detectOpenHandles",
    "test:ci": "npm run test -- --coverage && cat ./coverage/lcov.info | coveralls",
    "test:e2e": "env-cmd -e e2e --no-override start-server-and-test start http-get://localhost:3000 cy:e2e",
    "test:e2e:ci": "npm run test:e2e",
    "docs:readme": "gitdown ./.README.tmp --output-file ./README.md",
    "predocs:readme": "hercule ./README.source.md -o ./.README.tmp"
  },
  "dependencies": {
    "@sentry/browser": "^5.5.0",
    "@sentry/webpack-plugin": "^1.8.0",
    "@types/react": "^16.8.24",
    "@types/react-dom": "^16.8.5",
    "@types/react-router-dom": "^4.3.2",
    "autoprefixer": "^9.6.1",
    "awesome-typescript-loader": "^5.2.1",
    "case-sensitive-paths-webpack-plugin": "^2.2.0",
    "classnames": "^2.2.6",
    "clean-webpack-plugin": "^1.0.1",
    "copy-webpack-plugin": "^4.5.3",
    "core-js": "^2.6.4",
    "css-loader": "^3.1.0",
    "cssnano": "^4.1.10",
    "date-fns": "^2.0.0-beta.3",
    "env-cmd": "^9.0.0",
    "es-check": "^5.0.0",
    "extract-css-chunks-webpack-plugin": "^4.6.0",
    "file-loader": "^3.0.1",
    "gettext-utils": "^1.2.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "lioness": "^0.2.15",
    "markdown-loader": "^5.1.0",
    "postcss-custom-media": "^7.0.8",
    "postcss-custom-properties": "^9.0.2",
    "postcss-loader": "^3.0.0",
    "postcss-normalize": "^8.0.1",
    "raw-loader": "^3.1.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-hot-loader": "^4.12.10",
    "react-router-dom": "^5.0.0",
    "react-svg": "^10.0.0",
    "ts-node": "^8.3.0",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "typescript": "^3.5.3",
    "typescript-polyfills-generator": "^0.2.0",
    "webpack": "^4.39.1",
    "webpack-bundle-analyzer": "^3.4.1",
    "webpack-cli": "^3.3.6",
    "webpackbar": "^3.2.0"
  },
  "devDependencies": {
    "@cypress/webpack-preprocessor": "^4.1.0",
    "@storybook/addon-a11y": "~5.1.10",
    "@storybook/addon-actions": "~5.1.10",
    "@storybook/addon-console": "^1.2.0",
    "@storybook/addon-info": "~5.1.10",
    "@storybook/addon-knobs": "~5.1.10",
    "@storybook/addon-viewport": "~5.1.10",
    "@storybook/react": "~5.1.10",
    "@testing-library/cypress": "^4.0.5",
    "@testing-library/jest-dom": "^4.0.0",
    "@testing-library/react": "^8.0.7",
    "@types/classnames": "^2.2.9",
    "@types/jest": "^24.0.16",
    "@types/storybook__addon-actions": "^3.4.2",
    "@types/storybook__addon-knobs": "^5.0.3",
    "@types/storybook__react": "^4.0.1",
    "coveralls": "^3.0.5",
    "cypress": "^3.4.1",
    "gitdown": "^3.1.1",
    "hard-source-webpack-plugin": "^0.13.1",
    "hercule": "^4.1.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.7.0",
    "jest-date-mock": "^1.0.7",
    "prettier-config-trucknet-io": "^2.0.1",
    "react-docgen-typescript-loader": "~3.1.0",
    "rimraf": "^2.6.3",
    "start-server-and-test": "^1.7.11",
    "storybook-addon-i18n": "~5.1.9",
    "storybook-addon-themes": "^5.2.0",
    "stylelint": "^10.1.0",
    "stylelint-config-css-modules": "^1.4.0",
    "stylelint-config-idiomatic-order": "^7.0.3",
    "stylelint-config-prettier": "^5.2.0",
    "stylelint-config-standard": "^18.3.0",
    "trucknet-commit": "^1.5.3",
    "ts-jest": "^24.0.0",
    "tslint": "^5.16.0",
    "tslint-config-trucknet-io": "^1.8.2",
    "typescript-plugin-css-modules": "^1.2.1",
    "webpack-dev-server": "^3.3.1",
    "ws": "^7.1.1"
  }
}
